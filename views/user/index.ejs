          <ul>
            <% for(let i = 0; i < users.length; i++){ %>
                <li>
                    <a href="/user/<%=users[i]._id%>"><%=users[i].name%></a>
                </li>
            <% } %>
        </ul>
			</div>
			</div>
      </main>

	</header
