	<body>
		<header>
			<div class="container">
				<div class="header-1">
			<h1>Uploads</h1>
		</div>
		<div class="flex-container">

						<p class="navbutton"><a href="/home">Back to Home</a></p>
						<p class="navbutton"><a href="/uploads/new">Publish a new Tailgating Experience</a></p>
					</div>
				</div>
    <main>
			<div class="container-4">
			     <div class="t-adventures">
	<h2>List of Tailgating Expereinces</h2>
</div>
		<div class="box-8">
		<% for(let i = 0; i < Uploads.length; i++){ %>
			<div class="event"><img src = "/uploads/<%=Uploads[i].url%>" style="width:125px; height:100px;"></br>

				<a href="/uploads/<%=Uploads[i]._id%>"><%=Uploads[i].title%></a>
			</div>
		<% } %>
	</div>

</main>
	</body>
</html>
